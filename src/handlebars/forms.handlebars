											<input id="{{label-id}}" class="form-control input-sm iad-control-text" type="text" value="{{label-value}}">
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Target shape">Shape</span>
										</div>
										<div>
											<select id="{{shape-id}}" class="form-control input-sm iad-control-select">
												{{#each shape-choices}}
													<option value="{{this.value}}" {{#ifEqualTo this.value ../shape-value}}selected="true"{{/ifEqualTo}}>{{this.label}}</option>
												{{/each}}
											</select>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Target size">Size</span>
										</div>
										<div>
											<div class="input-group">
												<input id="{{size-id}}" class="form-control input-sm iad-control-number iad-control-integer" type="hidden" value="{{size-value}}">
												<span class="input-group-btn">													
													<button type="button" class="btn btn-sm btn-default iad-control-integer-minus iad-tooltip-control" data-toggle="tooltip" title="Decrease size">-</button>
													<button type="button" class="btn btn-sm btn-default iad-control-integer-plus iad-tooltip-control" data-toggle="tooltip" title="Increase size">+</button>
												</span>
											</div>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Target colour">Colour</span>
										</div>
										<div>
											<button id="{{color-id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{color-value}}">&nbsp;&nbsp;</button>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'profile-break'}}
								<div class="well well-sm">
									<div class="form-group iad-form-group">
										<div style="text-align:right">
											<button id="{{id}}" type="button" class="close iad-control-remove-break iad-tooltip-control" data-toggle="tooltip" title="Remove">&times;</button>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Break label">Label</span>
										</div>
										<div>
											<input id="{{label-id}}" class="form-control input-sm iad-control-text" type="text" value="{{label-value}}">
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Break colour">Colour</span>
										</div>
										<div>
											<button id="{{color-id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{color-value}}">&nbsp;&nbsp;</button>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}

							<!-- Menu Bar -->
							{{#ifEqualTo type 'menu-bar'}}
								<div class="well well-sm">
									<div class="form-group iad-form-group">
										<div style="text-align:right">
											<button id="{{id}}" type="button" class="close iad-control-remove-menu-item iad-tooltip-control" data-toggle="tooltip" title="Remove">&times;</button>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="{{label-description}}">Label</span>
										</div>
										<div>
											<input id="{{label-id}}" class="form-control input-sm iad-control-text" type="text" value="{{label-value}}">
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="{{func-description}}">Function</span>
										</div>
										<div>
											<div class="input-group">
												<input id="{{func-id}}" class="form-control input-sm iad-control-text" type="text" value="{{func-value}}">
												<div class="input-group-btn">
													<button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
													<ul class="dropdown-menu pull-right iad-dropdown-menu-replace">
														{{#each func-choices}}
															{{#if this.divider}}
																<li class="divider"></li>
															{{/if}}
															{{#if this.header}}
																<li class="dropdown-header">{{this.header}}</li>
															{{/if}}
															{{#if this.value}}
																<li class="iad-dropdown-option"><a href="#" data-value="{{this.value}}">{{this.label}}</a></li>
															{{/if}}
														{{/each}}
													</ul>
												</div>
											</div>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'menu-bar-add'}}
								<div class="form-group iad-form-group">
									<div style="text-align:right">
										<button id="{{id}}" type="button" class="btn btn-sm btn-primary iad-control-add-menu-item iad-tooltip-control" data-toggle="tooltip" title="Add a new item"><i class="fa fa-fw fa-plus"></i>&nbsp;&nbsp;New Item</button>
									</div>
								</div>
							{{/ifEqualTo}}

							<!-- Data Properties -->
							{{#ifEqualTo type 'data-textarea'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										<span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
									</div>
									<div>
										<textarea id="{{id}}" class="form-control input-sm iad-control-textarea iad-control-text iad-data-control">{{value}}</textarea>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'data-string'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										<span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
									</div>
									<div>
										<input id="{{id}}" class="form-control input-sm iad-control-text iad-data-control" type="text" {{#if this.value}}value="{{value}}"{{/if}} placeholder="{{placeholder}}">
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'data-boolean'}}
								<div class="form-group iad-form-group">
									<div class="checkbox">
										<label>
											<input id="{{id}}" type="checkbox" {{#ifEqualTo ../value 'true'}}checked{{/ifEqualTo}} class="iad-control-checkbox iad-data-control"><span class="control-label iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
										</label>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'data-text-dropdown-replace'}}
								<div class="form-group iad-form-group">
									<div class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-text iad-data-control" type="text" value="{{value}}">
											<div class="input-group-btn">
												<button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
												<ul class="dropdown-menu pull-right iad-dropdown-menu-replace">
													{{#each choices}}
														{{#if this.divider}}
															<li class="divider"></li>
														{{/if}}
														{{#if this.header}}
															<li class="dropdown-header">{{this.header}}</li>
														{{/if}}
														{{#if this.value}}
															<li class="iad-dropdown-option"><a href="#" data-value="{{this.value}}">{{this.label}}</a></li>
														{{/if}}
													{{/each}}
												</ul>
											</div>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'open-data-properties-form'}}
								<div class="form-group iad-form-group">
									<div style="text-align:right">
										<button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#iad-modal-data-properties">{{this.label}}</button>	
									</div>
								</div>
							{{/ifEqualTo}}
                            
                            {{#ifEqualTo type 'open-map-layers-form'}}
                            <div class="form-group iad-form-group">
                                <div class="control-label">
                                    <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                </div>
                                <div class="control-label">
                                    {{#if this.label}}
                                    <div style="margin-top: -0.5em;">
                                        <button type="button" class="btn btn-sm btn-default iad-tooltip-control" data-toggle="modal" data-target="#iad-modal-layer-manager" title="{{this.description}}" data-toggle="tooltip">
                                            <span class="fa-stack iad-layers-icon"><i class="fa fa-fw fa-stop fa-stack-1x"></i><i class="fa fa-fw fa-stop fa-stack-1x"></i><i class="fa fa-fw fa-stop fa-stack-1x"></i></span>
                                            <span>{{this.label}}</span>
                                        </button>
                                    </div>
                                    {{/if}}
                                    <!--<ul class="list-unstyled text-left">
                                        {{#each choices}}
                                        {{#if this.value}}
                                        <li>{{this.label}}</li>
                                        {{/if}}
                                        {{/each}}
                                    </ul>-->
                                </div>
                            </div>
                            {{/ifEqualTo}}

							<!-- Pyramid -->
							{{#ifEqualTo type 'pyramid-line-add'}}
								<div class="form-group iad-form-group">
									<div style="text-align:right">
										<button id="{{id}}" type="button" class="btn btn-sm btn-primary iad-control-add-pyramid-line iad-tooltip-control" data-toggle="tooltip" title="Add a new line"><i class="fa fa-fw fa-plus"></i>&nbsp;&nbsp;New Line</button>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'pyramid-line'}}
								<div class="well well-sm">
									<div class="form-group iad-form-group">
										<div style="text-align:right">
											<button id="{{id}}" type="button" class="close iad-control-remove-pyramid-line iad-tooltip-control" data-toggle="tooltip" title="Remove">&times;</button>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Data to be associated with the target">Data</span>
										</div>
										<div>
											<select id="{{data-id}}" class="form-control input-sm iad-control-select">
												{{#each data-choices}}
													<option value="{{this.value}}" {{#ifEqualTo this.value ../data-value}}selected="true"{{/ifEqualTo}}>{{this.label}}</option>
												{{/each}}
											</select>
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Line label">Label</span>
										</div>
										<div>
											<input id="{{label-id}}" class="form-control input-sm iad-control-text" type="text" value="{{label-value}}">
										</div>
									</div>

									<div class="form-group iad-form-group">
										<div class="control-label">
											<span class="iad-tooltip-control" data-toggle="tooltip" title="Line colour">Colour</span>
										</div>
										<div>
											<button id="{{color-id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{color-value}}">&nbsp;&nbsp;</button>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}

						{{/controls}} 
					</form>
				</div>	
			</div>
		</div>
	{{/forms}} 

