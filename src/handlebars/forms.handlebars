<div class="panel-group" id="{{id}}-accordion">

	{{#forms}} 
        {{#if description}}
        <div class="panel-body">
            <div class="iad-form-description">{{{description}}}</div>
        </div>
        {{/if}}

		<div class="panel panel-default" {{#if @last}}style="border-bottom-width:0px"{{/if}}>
			{{#if name}}
				<div style="cursor:pointer" class="panel-heading {{#isActive @index}} {{else}}collapsed{{/isActive}}" data-toggle="collapse" data-parent="#{{../id}}-accordion" href="#collapse-{{../id}}-{{@index}}"><span>&nbsp;{{{name}}}</span></div>
			{{/if}}

    		<div id="collapse-{{../id}}-{{@index}}" class="panel-collapse collapse 
    		{{#if name}}{{#isActive @index}}in{{/isActive}}{{else}}in{{/if}}
    		iad-collapse">

				<div class="panel-body">
					<form role="form" onsubmit="return false;">

						{{#controls}} 

							<!-- Map Palettes -->
							{{#ifEqualTo type 'map-palettes-legend-editor'}}
								<div class="form-group iad-form-group">
									<div id="map-palettes-legend-editor">

									</div>
								</div>
							{{/ifEqualTo}}

							{{#ifEqualTo type 'map-palettes-color-range-add'}}
								<div class="form-group iad-form-group">
									<div style="text-align:right">
										<button id="{{id}}" type="button" class="btn btn-sm btn-primary iad-control-add-color-range iad-tooltip-control" data-toggle="tooltip" title="Add a new colour"><i class="fa fa-fw fa-plus"></i></button>
									</div>
								</div>
							{{/ifEqualTo}}

							{{#ifEqualTo type 'map-palettes-color-range'}}
								<div class="form-group iad-form-group">
									<div class="input-group">
										<span class="btn-group">
											<button id="{{color-id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{color-value}}">&nbsp;&nbsp;</button>
											{{#noOfColorsGreaterThanTwo ../../controls}}
												<button id="{{id}}" type="button" class="btn btn-sm btn-default iad-control-remove-color-range iad-tooltip-control" data-toggle="tooltip" title="Remove">&times;</button>
											{{/noOfColorsGreaterThanTwo}}
										</span>
									</div>
								</div>
							{{/ifEqualTo}}

                            {{#ifEqualTo type 'map-palettes-color-scheme-add'}}
                                <div class="form-group iad-form-group">
                                    <div style="text-align:right">
                                        <button id="{{id}}" type="button" class="btn btn-sm btn-primary iad-control-add-color-scheme iad-tooltip-control" data-toggle="tooltip" title="Add a new colour"><i class="fa fa-fw fa-plus"></i></button>
                                    </div>
                                </div>
                            {{/ifEqualTo}}

							{{#ifEqualTo type 'map-palettes-color-scheme'}}
								<div class="form-group iad-form-group">
									<div class="input-group">
		      							<span class="input-group-btn">
											<button id="{{color-id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{color-value}}">&nbsp;&nbsp;</button>
										</span>
										<input id="{{for-id}}" class="form-control input-sm iad-control-text iad-tooltip-control" data-toggle="tooltip" title="Associate this colour with a legend label" type="text" value="{{for-value}}">
										{{#noOfColorsGreaterThanTwo ../../controls}}
			      							<span class="input-group-btn">
												<button id="{{id}}" type="button" class="btn btn-sm btn-default iad-control-remove-color-scheme iad-tooltip-control" data-toggle="tooltip" title="Remove">&times;</button>
											</span>
										{{/noOfColorsGreaterThanTwo}}
									</div>
								</div>
							{{/ifEqualTo}}

							<!-- Standard -->
							{{#ifTextControl type}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>

										{{#if PropertyGroup}}
											<div class="input-group">
												<input id="{{id}}" class="form-control input-sm iad-control-text" type="text" value="{{value}}">
												<span class="input-group-btn">
													<button type="button" class="btn btn-sm btn-default iad-update-config-btn"><i class="fa fa-fw fa-refresh"></i></button>
												</span>
											</div>
										{{else}}
											<input id="{{id}}" class="form-control input-sm iad-control-text" type="text" value="{{value}}">
										{{/if}}

									</div>
								</div>
							{{/ifTextControl}}
							{{#ifEqualTo type 'textarea'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
                                        {{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<textarea id="{{id}}" class="form-control input-sm iad-control-textarea iad-control-text">{{value}}</textarea>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'textarea-large'}}
								<div class="form-group iad-form-group">
									<div class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">
										<textarea id="{{id}}" class="form-control input-sm iad-control-textarea iad-control-text">{{value}}</textarea>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'label'}}
								<div class="form-group iad-form-group">
									<div class="iad-form-label">{{name}}</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'bold-label'}}
								<div class="form-group iad-form-group">
									<div class="iad-form-label"><b>{{name}}</b></div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'integer'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-number iad-control-integer" type="text" value="{{value}}">
											<span class="input-group-btn">
												<button type="button" class="btn btn-sm btn-default iad-control-integer-minus">-</button>
												<button type="button" class="btn btn-sm btn-default iad-control-integer-plus">+</button>
											</span>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'float'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-number iad-control-float" type="text" value="{{value}}">
											<span class="input-group-btn">
												<button type="button" class="btn btn-sm btn-default iad-control-integer-minus">-</button>
												<button type="button" class="btn btn-sm btn-default iad-control-integer-plus">+</button>
											</span>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'integer-counter'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-number iad-control-integer" type="hidden" value="{{value}}">
											<span class="btn-group">													
												<button type="button" class="btn btn-sm btn-default iad-control-integer-minus">-</button>
												<button type="button" class="btn btn-sm btn-default iad-control-integer-plus">+</button>
											</span>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'integer-select'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<select id="{{id}}" class="form-control input-sm iad-control-integer-select">
												<option value="-1" {{#ifEqualTo -1 value}}selected="true"{{/ifEqualTo}}>-</option>
												<option value="0" {{#ifEqualTo 0 value}}selected="true"{{/ifEqualTo}}>0</option>
												<option value="1" {{#ifEqualTo 1 value}}selected="true"{{/ifEqualTo}}>1</option>
												<option value="2" {{#ifEqualTo 2 value}}selected="true"{{/ifEqualTo}}>2</option>
												<option value="3" {{#ifEqualTo 3 value}}selected="true"{{/ifEqualTo}}>3</option>
												<option value="4" {{#ifEqualTo 4 value}}selected="true"{{/ifEqualTo}}>4</option>
												<option value="5" {{#ifEqualTo 5 value}}selected="true"{{/ifEqualTo}}>5</option>
												<option value="6" {{#ifEqualTo 6 value}}selected="true"{{/ifEqualTo}}>6</option>
												<option value="7" {{#ifEqualTo 7 value}}selected="true"{{/ifEqualTo}}>7</option>
												<option value="8" {{#ifEqualTo 8 value}}selected="true"{{/ifEqualTo}}>8</option>
												<option value="9" {{#ifEqualTo 9 value}}selected="true"{{/ifEqualTo}}>9</option>
												<option value="10" {{#ifEqualTo 10 value}}selected="true"{{/ifEqualTo}}>10</option>
										</select>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'float-counter'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-number iad-control-float" type="hidden" value="{{value}}">
											<span class="btn-group">													
												<button type="button" class="btn btn-sm btn-default iad-control-float-minus">-</button>
												<button type="button" class="btn btn-sm btn-default iad-control-float-plus">+</button>
											</span>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'boolean'}}
								<div class="form-group iad-form-group">
										<div class="checkbox">
											<label>
												<input id="{{id}}" type="checkbox" {{#ifEqualTo this.value 'true'}}checked{{/ifEqualTo}} {{#ifEqualTo this.value true}}checked{{/ifEqualTo}} class="iad-control-checkbox"><span class="control-label iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
											</label>
										</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'select'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
                                        <input type="hidden" id="{{id}}-current" value="{{value}}" />
										<select id="{{id}}" class="form-control input-sm iad-control-select">
                                            {{#ifEndsWith id 'font-family'}}
                                            {{#each choices}}
                                            <option value="{{this.value}}" style="font-family: {{this.value}};" {{#ifEqualTo this.value ../value}} selected="true" {{/ifEqualTo}}>{{this.label}}</option>
                                            {{/each}}
                                            {{else}}
											{{#each choices}}
												<option value="{{this.value}}" {{#ifEqualTo this.value ../value}}selected="true"{{/ifEqualTo}}>{{this.label}}</option>
											{{/each}}
                                            {{/ifEndsWith}}
										</select>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'colour'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>

										{{#if PropertyGroup}}
											<span class="btn-group">
												<button id="{{id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{value}}">&nbsp;&nbsp;</button> 
												<button type="button" class="btn btn-sm btn-default iad-update-config-btn"><i class="fa fa-fw fa-refresh"></i></button>
											</span>
										{{else}}
											<button id="{{id}}" class="btn btn-sm btn-default iad-control-color-swatch" type="button" style="background-color:{{value}}">&nbsp;&nbsp;</button> 
										{{/if}}

									</div>
								</div>
							{{/ifEqualTo}}

                            {{#ifEqualTo type 'colour-dropdown'}}
                                <div class="form-group iad-form-group">
                                    <div>
                                        <div class="input-group">
                                            <div class="input-group-btn" style="vertical-align:bottom">
                                                <button id="{{id}}" type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown">{{name}}&nbsp;<span class="caret"></span></button>
                                                <ul class="dropdown-menu">
                                                    {{#each choices}}
                                                        <li class="iad-dropdown-option iad-dropdown-menu-colorpalette">
                                                        <a href="#" data-value="{{this.value}}">
                                                            {{#each colors}}
                                                                <span style="background:{{this}};border: 1px solid #ccc;">&nbsp;&nbsp;</span>
                                                            {{/each}}
                                                        </a></li>
                                                    {{/each}}
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {{/ifEqualTo}}

							{{#ifEqualTo type 'text'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
                                        {{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<input id="{{id}}" class="form-control input-sm iad-control-text" type="text" value="{{value}}">
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'range'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
                                        {{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
                                        <div class="input-group">
										  <input id="{{id}}" class="iad-control-range" type="range" value="{{value}}" min="{{min}}" max="{{max}}" step="{{step}}">
                                          <div class="input-group-addon iad-range-value">{{value}}</div>
                                        </div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'text-dropdown-replace'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-text" type="text" value="{{value}}">
											<div class="input-group-btn">
												<button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
												<ul class="dropdown-menu pull-right iad-dropdown-menu-replace">
													{{#each choices}}
														{{#if this.divider}}
															<li class="divider"></li>
														{{/if}}
														{{#if this.header}}
															<li class="dropdown-header">{{this.header}}</li>
														{{/if}}
														{{#if this.value}}
															<li class="iad-dropdown-option"><a href="#" data-value="{{this.value}}">{{this.label}}</a></li>
														{{/if}}
													{{/each}}
												</ul>
											</div>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'text-dropdown-append'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<input id="{{id}}" class="form-control input-sm iad-control-text" type="text" value="{{value}}">
											<div class="input-group-btn">
												<button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
												<ul class="dropdown-menu pull-right iad-dropdown-menu-append">
													{{#each choices}}
														{{#if this.divider}}
															<li class="divider"></li>
														{{/if}}
														{{#if this.header}}
															<li class="dropdown-header">{{this.header}}</li>
														{{/if}}
														{{#if this.value}}
															<li class="iad-dropdown-option"><a href="#" data-value="{{this.value}}">{{this.label}}</a></li>
														{{/if}}
													{{/each}}
												</ul>
											</div>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}
							{{#ifEqualTo type 'textarea-dropdown-append'}}
								<div class="form-group iad-form-group">
									<div class="control-label">
										{{#ifStartsWith description 'popover:'}}
                                        <span class="iad-popover-control"><i class="hover-opaque fa fa-fw fa-question-circle iad-popover" data-toggle="popover" data-trigger="click" data-content="{{substring description 8}}" data-title="{{name}}" data-container="body"></i> {{name}}</span>
                                        {{else}}
                                        <span class="iad-tooltip-control" data-toggle="tooltip" title="{{description}}">{{name}}</span>
                                        {{/ifStartsWith}}
									</div>
									<div>
										<div class="input-group">
											<textarea id="{{id}}" class="form-control input-sm iad-control-textarea iad-control-text">{{value}}</textarea>
											<div class="input-group-btn" style="vertical-align:bottom">
												<button type="button" class="btn btn-sm btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
												<ul class="dropdown-menu pull-right iad-dropdown-menu-append">
													{{#each choices}}
														{{#if this.divider}}
															<li class="divider"></li>
														{{/if}}
														{{#if this.header}}
															<li class="dropdown-header">{{this.header}}</li>
														{{/if}}
														{{#if this.value}}
															<li class="iad-dropdown-option"><a href="#" data-value="{{this.value}}">{{this.label}}</a></li>
														{{/if}}
													{{/each}}
												</ul>
											</div>
										</div>
									</div>
								</div>
							{{/ifEqualTo}}

							{{#ifEqualTo type 'separator'}}
								<div class="form-group iad-form-group">
									<div style="border-bottom:1px solid #cccccc;margin:40px 0px 40px 0px"></div>
								</div>
							{{/ifEqualTo}}

							<!-- Tables -->
							{{#ifEqualTo type 'column'}}
								<div id="{{id}}" class="well well-sm iad-sortable">

